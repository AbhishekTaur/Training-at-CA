package fileDelete;

import java.io.File;

public class Filo {

	public static void main(String[] args) {
		String path = "C:/Agent/NIKKI_COPY_COPY1.txt";
		File file = new File(path);
        String message;
        
        if(file.delete()){
			message = file.getName() + " is deleted!";
		}else{
			message = "Delete operation is failed.";
		}
        
        System.out.println(message);

	}

}
